<div class="container">
    {{#each productos}}
        <div class="product-container">
            <p>{{this.title}}</p>
            <p>{{this.description}}</p>
            <p>{{this.code}}</p>
            <p>{{this.price}}</p>
            <p>{{this.status}}</p>
            <p>{{this.thumbnail}}</p>
            <p>{{this.stock}}</p>
            <p>{{this.category}}</p>
        </div>
    {{/each}}
</div>
